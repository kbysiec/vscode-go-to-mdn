{
  "name": "vscode-go-to-mdn",
  "displayName": "Go to MDN",
  "description": "Browse MDN web docs directly from command palette",
  "version": "1.1.3",
  "publisher": "kbysiec",
  "author": {
    "name": "Kamil Bysiec",
    "email": "kamil.bysiec@agileplayers.com",
    "url": "https://github.com/kbysiec"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "MDN",
    "Mozilla",
    "Browser"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Go to MDN",
      "properties": {
        "goToMDN.githubPersonalAccessToken": {
          "type": "string",
          "default": null,
          "description": "Allows to increase number of requests to API from 60 (unauthenticated) to 5000 (authenticated) per hour."
        },
        "goToMDN.shouldDisplayFlatList": {
          "type": "boolean",
          "default": true,
          "description": "Should all anchors be displayed as flat list at one level. Requires provided github personal access token."
        }
      }
    },
    "commands": [
      {
        "command": "goToMDN.browse",
        "title": "Go to MDN: Browse"
      },
      {
        "command": "goToMDN.clearCache",
        "title": "Go to MDN: Clear cache"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": " rimraf ./dist/ && tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./dist/test/runTest.js",
    "cov:fixpaths": "gulp fixCoverageReportSrcPaths"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.3",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.20",
    "@types/node-fetch": "^2.5.7",
    "@types/sinon": "^9.0.4",
    "@types/vscode": "1.43.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "decache": "^4.6.0",
    "fetch-mock": "^9.10.3",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-string-replacer": "^1.2.2",
    "istanbul": "^0.4.5",
    "mocha": "^8.1.1",
    "proxyquire": "^2.1.3",
    "remap-istanbul": "^0.13.0",
    "sinon": "^9.0.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6",
    "vscode-test": "1.3.0"
  },
  "dependencies": {
    "debounce": "^1.2.0",
    "node-fetch": "^2.6.0",
    "open": "^7.0.4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kbysiec/vscode-go-to-mdn"
  },
  "bugs": {
    "url": "https://github.com/kbysiec/vscode-go-to-mdn/issues"
  },
  "icon": "img/logo.png"
}
